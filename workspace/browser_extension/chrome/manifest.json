{
  "manifest_version": 2,

  "name": "Dynotate",
  "description": "Dynamic web application annotations.",
  "version": "1.0",

  "permissions": [
    "activeTab", "tabs", "https://*/", "http://*/"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
 "background": {
    "scripts": ["eventpage.js"], 
	"persistent": false
  }  
}
